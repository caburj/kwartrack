{
  "name": "expensif",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "panda codegen && npx @edgedb/generate@0.4.1 edgeql-js",
    "dev": "next dev",
    "dev-turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "prettier": "prettier --write . --ignore-path .prettierignore",
    "update-deps": "ncu --upgrade",
    "migrate-all": "tsc -p tsconfig.scripts.json && node compiled/scripts/migrate-all.js",
    "invite-user": "tsc -p tsconfig.scripts.json && node compiled/scripts/invite-user.js",
    "create-first-user": "tsc -p tsconfig.scripts.json && node compiled/scripts/create-first-user.js",
    "migrate": "edgedb migration create && pnpm run migrate-all && npx @edgedb/generate edgeql-js && npx @edgedb/generate queries"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.4",
    "@radix-ui/colors": "3.0.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/themes": "2.0.3",
    "@tanstack/react-query": "^4.32.6",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@uidotdev/usehooks": "^2.4.1",
    "arg": "^5.0.2",
    "chart.js": "^4.4.1",
    "chartjs-plugin-autocolors": "^0.2.2",
    "cmdk": "^0.2.0",
    "disturb": "^1.0.9",
    "dotenv": "^16.3.2",
    "edgedb": "^1.4.1",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "next": "14.1.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^4.25.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.0.1",
    "react-loading-skeleton": "^3.3.1",
    "sass": "^1.70.0",
    "sonner": "^1.3.1",
    "typescript": "5.3.3",
    "valibot": "^0.26.0"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.4.1",
    "@pandacss/dev": "^0.27.3",
    "@tsconfig/node-lts": "^20.1.1",
    "@types/react-datepicker": "^4.19.5"
  }
}
